<?xml version="1.0" standalone="yes"?>
<xs:schema id="dsClientes" targetNamespace="http://www.tempuri.org/dsClientes.xsd" xmlns:mstns="http://www.tempuri.org/dsClientes.xsd" xmlns="http://www.tempuri.org/dsClientes.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="dsClientes" msdata:IsDataSet="true" msdata:Locale="pt-BR">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="Clientes">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="CargoDoContato" type="xs:string" minOccurs="0" />
              <xs:element name="CEP" type="xs:string" minOccurs="0" />
              <xs:element name="Cidade" type="xs:string" minOccurs="0" />
              <xs:element name="CódigoDoCliente" type="xs:string" />
              <xs:element name="Endereço" type="xs:string" minOccurs="0" />
              <xs:element name="Fax" type="xs:string" minOccurs="0" />
              <xs:element name="NomeDaEmpresa" type="xs:string" minOccurs="0" />
              <xs:element name="NomeDoContato" type="xs:string" minOccurs="0" />
              <xs:element name="País" type="xs:string" minOccurs="0" />
              <xs:element name="Região" type="xs:string" minOccurs="0" />
              <xs:element name="Telefone" type="xs:string" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
    <xs:unique name="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:Clientes" />
      <xs:field xpath="mstns:CódigoDoCliente" />
    </xs:unique>
  </xs:element>
</xs:schema>